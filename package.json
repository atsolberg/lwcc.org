{
  "name": "lwcc.org",
  "version": "1.0.0",
  "description": "Repo for the lwcc.org website",
  "scripts": {
    "start": "parcel ./src/scripts/header-footer.js --dist-dir public",
    "dev": "parcel ./src/scripts/header-footer.js --dist-dir public",
    "prebuild": "shx rm -rf public/*",
    "build": "parcel build ./src/scripts/header-footer.js --dist-dir public --public-url /public/",
    "vercel-build": "parcel build ./src/header-footer.html --dist-dir public",
    "dev-hf-html": "parcel ./src/header-footer.html --dist-dir public",
    "tw-build": "npx tailwindcss -i ./src/input.css -o ./public/tw-output.css",
    "tw-watch": "npx tailwindcss -i ./src/input.css -o ./public/tw-output.css --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write 'src/**/*.{js,scss}'",
    "prettier:check": "prettier --list-different 'src/**/*.{js,scss}'",
    "lint": "eslint src --cache",
    "lint:errors": "eslint src --cache --quiet",
    "validate": "npm-run-all -l prettier:check lint:errors test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atsolberg/lwcc.org.git"
  },
  "keywords": [
    "lwcc",
    "Living World Christian Center"
  ],
  "author": "Aaron Solberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atsolberg/lwcc.org/issues"
  },
  "homepage": "https://github.com/atsolberg/lwcc.org#readme",
  "dependencies": {
    "@babel/preset-env": "^7.18.9",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "axios": "^0.27.2",
    "babel-eslint": "^10.0.2",
    "babel-plugin-emotion": "^11.0.0",
    "chalk": "^4.1.2",
    "classnames": "^2.3.1",
    "core-js": "^3.24.1",
    "custom-event-polyfill": "^1.0.7",
    "eslint-plugin-jest": "^26.7.0",
    "js-cookie": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-async-script": "^1.2.0",
    "react-dom": "^18.2.0",
    "url-polyfill": "^1.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@parcel/resolver-glob": "^2.6.2",
    "@parcel/transformer-sass": "^2.6.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.15",
    "babel-core": "^6.26.3",
    "babel-jest": "^28.1.3",
    "buffer": "^6.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.3.2",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "shx": "^0.3.4",
    "tailwindcss": "^3.2.4"
  }
}
