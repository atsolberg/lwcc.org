ningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/ABTestsProvider.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/App.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/btn-group/BtnGroup.js",["282"],"import React from 'react';\nimport cx from 'classnames';\nimport { arrayOf, shape, string, number, func, oneOfType } from 'prop-types';\n\n/**\n * Renders a bs btn group with buttons for each option provided.\n */\nconst BtnGroup = ({ options, selected, onSelect, className, ...rest }) => (\n  <div className={cx(className, 'btn-group')} {...rest}>\n    {options.map(o => (\n      <button\n        type=\"button\"\n        key={o.value}\n        className={cx('btn', 'btn-default', {\n          active: o.value === selected,\n        })}\n        onClick={() => onSelect(o.value)}\n      >\n        {o.label}\n      </button>\n    ))}\n  </div>\n);\n\nBtnGroup.displayName = 'BtnGroup';\nBtnGroup.propTypes = {\n  options: arrayOf(\n    shape({\n      label: string.isRequired,\n      value: oneOfType([string, number]).isRequired,\n    })\n  ).isRequired,\n  selected: oneOfType([string, number]).isRequired,\n  onSelect: func.isRequired,\n};\n\nexport default BtnGroup;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/button/index.js",["283","284","285","286","287"],"import React from 'react';\nimport BsButton from 'react-bootstrap/es/Button';\nimport Spinner from '../spinner/Spinner';\n\n/**\n * Use this to produce buttons instead of html button tags.\n */\nconst Button = ({ label, busy, busyLabel, disabled, children, ...rest }) => {\n  const btnLabel = busy && busyLabel ? busyLabel : label || children;\n\n  return (\n    <BsButton disabled={busy || disabled} {...rest}>\n      {busy && <Spinner className=\"bump-left-sm\" />}\n      {btnLabel}\n    </BsButton>\n  );\n};\n\nexport default Button;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/footer/Footer.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/footer/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/ListFormGroup.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/Select.js",["288","289","290","291","292","293","294","295","296","297","298","299","300"],"import React, { Component } from 'react';\nimport cx from 'classnames';\nimport FormLabel from 'react-bootstrap/es/FormLabel';\nimport FormGroup from 'react-bootstrap/es/FormGroup';\n\nimport { rif } from '../../util/jsx';\n\nimport HtmlId from '../html-id/HtmlId';\n\n/**\n * A component that renders a bootstrap form group with a select input.\n * Supports error/validation visualization.\n */\nclass Select extends Component {\n  render() {\n    const {\n      className,\n      children,\n      desc,\n      error,\n      id,\n      label,\n      required,\n      showErrorMessaging,\n      'aria-describedby': ariaDescribedby = this.ariaDescribedby,\n      groupClasses,\n      options,\n      inputRef,\n      ...rest\n    } = this.props;\n\n    const dynamicAttrs = { 'aria-describedby': ariaDescribedby, ...rest };\n\n    return (\n      <HtmlId id={id}>\n        {htmlId => (\n          <FormGroup\n            className={cx(groupClasses, { 'has-error': !!error })}\n            controlId={htmlId}\n          >\n            {label && (\n              <>\n                {required && <span className=\"required-label\">*</span>}\n                <FormLabel>{label}</FormLabel>\n                {desc && <div className=\"control-desc\">{desc}</div>}\n              </>\n            )}\n            <select\n              id={htmlId}\n              className={cx('form-control', className)}\n              ref={inputRef || (c => (this.input = c))}\n              {...dynamicAttrs}\n            >\n              {options &&\n                options.map(o => (\n                  <option key={o.value} value={o.value}>\n                    {o.label}\n                  </option>\n                ))}\n              {children}\n            </select>\n            {rif(\n              error && showErrorMessaging,\n              <p\n                className=\"error-message\"\n                dangerouslySetInnerHTML={{ __html: error }}\n                id={ariaDescribedby}\n                role=\"alert\"\n              />\n            )}\n          </FormGroup>\n        )}\n      </HtmlId>\n    );\n  }\n}\n\nexport default Select;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/ToggleInput.js",["301","302","303"],"import React from 'react';\nimport cx from 'classnames';\nimport { bool, func, string, any } from 'prop-types';\n\n/**\n * Renders a bs btn group with two 'yes', 'no' buttons.\n * Driven by two radio inputs.\n */\nfunction ToggleInput({ label, name, value, onChange, className }) {\n  const inputProps = { onChange };\n\n  if (name) inputProps.name = name;\n\n  return (\n    <div className={cx(className, 'form-group')}>\n      <label className=\"control-label\">{label}</label>\n      <div className=\"btn-group db clearfix\">\n        <label\n          className={`btn btn-sm btn-default${\n            !!value === true ? ' active' : ''\n          }`}\n        >\n          <input\n            type=\"radio\"\n            value\n            checked={!!value === true}\n            {...inputProps}\n          />\n          <span className={!!value === true ? 'text-success' : ''}>Yes</span>\n        </label>\n        <label\n          className={`btn btn-sm btn-default${\n            !!value !== true ? ' active' : ''\n          }`}\n        >\n          <input\n            type=\"radio\"\n            value={false}\n            checked={!!value !== true}\n            {...inputProps}\n          />\n          <span className={!!value !== true ? 'text-danger' : ''}>No</span>\n        </label>\n      </div>\n    </div>\n  );\n}\nToggleInput.propTypes = {\n  label: string.isRequired,\n  value: bool.isRequired,\n  onChange: func.isRequired,\n  name: string,\n  className: any,\n};\n\nexport default ToggleInput;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/ZipInput.js",["304","305","306","307","308","309"],"import React from 'react';\n\nimport TextInput from './text/TextInput';\n\n/**\n * A component that renders a bootstrap form group with a zip code number input.\n */\nfunction ZipInput({\n  label,\n  id,\n  error,\n  required,\n  children,\n  showErrorMessaging = true,\n  ...rest\n}) {\n  return (\n    <TextInput\n      id={id}\n      label={label}\n      error={error}\n      pattern={'\\\\d{1,5}'}\n      required={required}\n      showErrorMessaging={showErrorMessaging}\n      {...rest}\n    >\n      {children}\n    </TextInput>\n  );\n}\n\nexport default ZipInput;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/checkbox/Checkbox.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/checkbox/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/radio/RadioButton.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/radio/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/text/TextInput.js",["310","311","312"],"import React, { useRef } from 'react';\nimport {\n  node,\n  oneOfType,\n  func,\n  string,\n  bool,\n  oneOf,\n  any,\n  shape,\n} from 'prop-types';\nimport cx from 'classnames';\n\nimport FormLabel from 'react-bootstrap/es/FormLabel';\nimport FormControl from 'react-bootstrap/es/FormControl';\nimport FormGroup from 'react-bootstrap/es/FormGroup';\n\nimport HtmlId from '../../html-id/HtmlId';\n\nimport { uuid } from '../../../util/string';\nimport styles from './styles';\n\nfunction TextInput({\n  type = 'text',\n  desc,\n  error,\n  id,\n  label,\n  required = false,\n  showErrorMessaging = true,\n  'aria-describedby': ariaDescribedby,\n  groupClasses,\n  children,\n  ...rest\n}) {\n  const errorDescriptorRef = useRef(uuid());\n\n  const describers = [];\n  const showError = error && showErrorMessaging;\n\n  if (ariaDescribedby) describers.push(ariaDescribedby);\n  if (showError) describers.push(errorDescriptorRef.current);\n\n  const dynamicAttrs = { ...rest };\n\n  if (describers.length) {\n    dynamicAttrs['aria-describedby'] = describers.join(' ');\n  }\n\n  if (error) {\n    dynamicAttrs['aria-invalid'] = true;\n  }\n\n  return (\n    <HtmlId id={id}>\n      {htmlId => (\n        <FormGroup\n          className={cx(groupClasses, { 'has-error': !!error })}\n          css={styles}\n          controlId={htmlId}\n        >\n          {label && (\n            <>\n              {required && <span className=\"required-label\">*</span>}\n              <FormLabel>{label}</FormLabel>\n              {desc && <div className=\"control-desc\">{desc}</div>}\n            </>\n          )}\n          <FormControl type={type} required={required} {...dynamicAttrs} />\n          {children}\n          <p\n            className={cx('error-message', {\n              hide: !showError,\n            })}\n            dangerouslySetInnerHTML={{ __html: error }}\n            id={errorDescriptorRef.current}\n            role=\"alert\"\n            aria-atomic\n          />\n        </FormGroup>\n      )}\n    </HtmlId>\n  );\n}\nTextInput.propTypes = {\n  type: oneOf(['text', 'number', 'email', 'search']),\n  desc: node,\n  label: node,\n  id: string,\n  error: string,\n  required: bool,\n  inputRef: oneOfType([func, shape({ current: any })]),\n  showErrorMessaging: bool,\n};\n\nexport default TextInput;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/text/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/textarea/TextArea.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/forms/textarea/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/header/Header.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/header/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/html-id/HtmlId.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/icons/LoadingBars.js",["313","314","315"],"import React from 'react';\nimport cx from 'classnames';\n\nimport v from '../../../styles/variables';\n\nfunction LoadingBars({\n  width = '40',\n  color = v.primary,\n  className = '',\n  ...rest\n}) {\n  return (\n    <svg\n      className={cx(className)}\n      x=\"0\"\n      y=\"0\"\n      viewBox=\"0 0 24 30\"\n      enableBackground=\"new 0 0 50 50\"\n      width={width}\n      {...rest}\n    >\n      <rect x=\"0\" y=\"10\" width=\"4\" height=\"10\" fill={color} opacity=\"0.2\">\n        <animate\n          attributeName=\"opacity\"\n          attributeType=\"XML\"\n          values=\"0.2; 1; .2\"\n          begin=\"0s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n        <animate\n          attributeName=\"height\"\n          attributeType=\"XML\"\n          values=\"10; 20; 10\"\n          begin=\"0s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n        <animate\n          attributeName=\"y\"\n          attributeType=\"XML\"\n          values=\"10; 5; 10\"\n          begin=\"0s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n      </rect>\n      <rect x=\"8\" y=\"10\" width=\"4\" height=\"10\" fill={color} opacity=\"0.2\">\n        <animate\n          attributeName=\"opacity\"\n          attributeType=\"XML\"\n          values=\"0.2; 1; .2\"\n          begin=\"0.15s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n        <animate\n          attributeName=\"height\"\n          attributeType=\"XML\"\n          values=\"10; 20; 10\"\n          begin=\"0.15s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n        <animate\n          attributeName=\"y\"\n          attributeType=\"XML\"\n          values=\"10; 5; 10\"\n          begin=\"0.15s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n      </rect>\n      <rect x=\"16\" y=\"10\" width=\"4\" height=\"10\" fill={color} opacity=\"0.2\">\n        <animate\n          attributeName=\"opacity\"\n          attributeType=\"XML\"\n          values=\"0.2; 1; .2\"\n          begin=\"0.3s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n        <animate\n          attributeName=\"height\"\n          attributeType=\"XML\"\n          values=\"10; 20; 10\"\n          begin=\"0.3s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n        <animate\n          attributeName=\"y\"\n          attributeType=\"XML\"\n          values=\"10; 5; 10\"\n          begin=\"0.3s\"\n          dur=\"0.6s\"\n          repeatCount=\"indefinite\"\n        />\n      </rect>\n    </svg>\n  );\n}\n\nexport default LoadingBars;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/loadable/Loadable.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/loadable/LoadableGlass.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/main/Main.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/main/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/mask/Mask.js",["316","317","318"],"import React, { useState } from 'react';\nimport { bool, string, any } from 'prop-types';\nimport cx from 'classnames';\n\n/**\n * Renders children normally, unless `excerpt` is true, then it\n * will use a max height, fade out the rest, and show a 'continue'\n * control that when clicked, reveals the rest.\n * Supports mobile only behavior.\n */\nfunction Mask({\n  excerpt, // should we even try\n  copy = 'Continue Reading',\n  mobile = false, // excerpt for mobile only\n  className = '',\n  children,\n}) {\n  const [expanded, setExpanded] = useState(false);\n\n  const flavor = mobile ? 'm-maskable' : 'maskable';\n\n  const classes = cx(className, { [flavor]: excerpt, unmasked: expanded });\n\n  return (\n    <div className={classes}>\n      {children}\n      {excerpt && !expanded && (\n        <div className=\"maskable-mask\">\n          <div className=\"maskable-mask-fade\" />\n          <a onClick={() => setExpanded(true)}>{copy}</a>\n        </div>\n      )}\n    </div>\n  );\n}\nMask.propTypes = {\n  excerpt: bool,\n  copy: string,\n  mobile: bool,\n  className: any,\n  children: any,\n};\n\nexport default Mask;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/paging/Paging.js",["319","320","321","322","323","324","325","326","327","328","329","330","331","332","333"],"import React from 'react';\nimport cx from 'classnames';\nimport { func, number, shape } from 'prop-types';\n\nimport { spacing } from '../../util/constants';\nimport styles from './styles';\n\n/**\n * Builds an array of five numbers representing individual page\n * links for paging.\n * @param {number} page - the current page\n * @param {number} totalPages - total number of pages\n * @return {Array} - the paging links numbers\n */\nexport function buildPaging(page, totalPages) {\n  const pages = [];\n\n  if (totalPages < 5) {\n    for (let i = 1; i <= totalPages; i++) pages.push(i);\n  } else {\n    let rightStop = page + 2;\n    if (page < 3) rightStop += Math.min(3, totalPages) - page;\n\n    let leftStop = page - 2;\n    if (page > totalPages - 2) leftStop -= page + 2 - totalPages;\n\n    for (let i = page; i > 0 && i >= leftStop; i--) pages.unshift(i);\n    for (let i = page + 1; i <= totalPages && i <= rightStop; i++)\n      pages.push(i);\n  }\n\n  return pages;\n}\n\n/**\n * Renders a block level paging component next, prev, first, last links\n * as well as some individual page links.\n */\nfunction Paging({ paging: { page, size, total }, onPage, ...rest }) {\n  let hasNext = false;\n  let hasPrev = false;\n  const totalPages = Math.ceil(total / size);\n\n  if (page !== 1) hasPrev = true;\n  if (page !== totalPages) hasNext = true;\n\n  const pages = buildPaging(page, totalPages);\n  const hasFirst = pages[0] > 1;\n  const hasLast = pages[pages.length - 1] < totalPages;\n\n  return (\n    <div css={styles} className=\"paging\" role=\"region\" {...rest}>\n      <button\n        type=\"button\"\n        className={cx('paging-link paging-link--prev btn-link', {\n          disabled: !hasPrev,\n        })}\n        onClick={() => onPage(page - 1)}\n        disabled={!hasPrev}\n      >\n        <i className=\"fa fa-chevron-left\" aria-hidden=\"true\" />{' '}\n        <span className=\"paging-link-label\">PREV</span>\n      </button>\n      <button\n        type=\"button\"\n        className={cx('paging-link paging-link--next btn-link', {\n          disabled: !hasNext,\n        })}\n        onClick={() => onPage(page + 1)}\n        disabled={!hasNext}\n      >\n        <span className=\"paging-link-label\">NEXT</span>{' '}\n        <i className=\"fa fa-chevron-right\" aria-hidden=\"true\" />\n      </button>\n\n      {/* DESKTOP VERSION */}\n      <ul className=\"paging-pages list-inline hidden-xs\">\n        {hasFirst && [\n          <li key=\"first-link\">\n            <a className=\"paging-link\" onClick={() => onPage(1)}>\n              {1}\n            </a>\n          </li>,\n          <li key=\"first-dots\">{spacing.ellipsis}</li>,\n        ]}\n        {pages.map(p => {\n          if (p === page)\n            return (\n              <li key={p} aria-current=\"page\">\n                {p}\n              </li>\n            );\n          return (\n            <li key={p}>\n              <a className=\"paging-link\" onClick={() => onPage(p)}>\n                {p}\n              </a>\n            </li>\n          );\n        })}\n        {hasLast && [\n          <li key=\"last-dots\">{spacing.ellipsis}</li>,\n          <li key=\"last-link\">\n            <a className=\"paging-link\" onClick={() => onPage(totalPages)}>\n              {totalPages}\n            </a>\n          </li>,\n        ]}\n      </ul>\n\n      {/* MOBILE VERSION */}\n      <ul className=\"paging-pages list-inline visible-xs\">\n        {page === totalPages && [\n          <li key=\"first-link-m\">\n            <a className=\"paging-link\" onClick={() => onPage(1)}>\n              {1}\n            </a>\n          </li>,\n          <li key=\"first-slash\">/</li>,\n        ]}\n        <li>{page}</li>\n        {page < totalPages && [\n          <li key=\"last-slash\">/</li>,\n          <li key=\"last-link-m\">\n            <a className=\"paging-link\" onClick={() => onPage(totalPages)}>\n              {totalPages}\n            </a>\n          </li>,\n        ]}\n      </ul>\n    </div>\n  );\n}\nPaging.propTypes = {\n  paging: shape({\n    page: number.isRequired,\n    size: number.isRequired,\n    total: number.isRequired,\n  }).isRequired,\n  onPage: func.isRequired,\n};\n\nexport default Paging;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/paging/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/paging-header/PagingHeader.js",["334"],"import React from 'react';\nimport { arrayOf, bool, func, number, oneOf, shape, string } from 'prop-types';\n\nimport Dropdown from 'react-bootstrap/es/Dropdown';\nimport DropdownToggle from 'react-bootstrap/es/DropdownToggle';\nimport DropdownMenu from 'react-bootstrap/es/DropdownMenu';\nimport DropdownItem from 'react-bootstrap/es/DropdownItem';\n\nimport useHtmlId from '../../hooks/useHtmlId';\nimport styles from './styles';\n\n/**\n * Renders a block level paging component showing left aligned\n * paged stats: ('1 - 8 of 551 Reviews') and right aligned sorting control.\n */\nconst PagingHeader = ({\n  id = 'paging-header',\n  paging: { page, size, total },\n  sorting,\n  onSort,\n  type,\n}) => {\n  const htmlId = useHtmlId();\n  const end = page * size;\n  const start = end - (size - 1);\n  const current = sorting ? sorting.options.find(o => o.selected) : null;\n  const label = sorting ? (\n    <span className=\"paging-header-current hidden-xs\">{current.label}</span>\n  ) : null;\n\n  return (\n    <div css={styles} id={id}>\n      <span className=\"paging-header-count\">\n        {start} - {Math.min(end, total)} of {total}\n        {type && (\n          <>\n            {' '}\n            <span>{type}</span>\n          </>\n        )}\n      </span>\n      {!!sorting && (\n        <div className=\"dib pull-right\">\n          <label\n            htmlFor={htmlId}\n            id={`${htmlId}-label`}\n            className=\"selector-label\"\n          >\n            Sort by:\n          </label>\n\n          <Dropdown\n            id={`${id}-dropdown`}\n            className=\"paging-header-sort\"\n            onSelect={onSort}\n          >\n            <DropdownToggle id={`${htmlId}-toggle`} bsStyle=\"link\">\n              {label}\n            </DropdownToggle>\n            <DropdownMenu>\n              {sorting.options.map(o => (\n                <DropdownItem\n                  key={o.value}\n                  eventKey={o.value}\n                  active={o.value === current.value}\n                >\n                  {o.label}\n                </DropdownItem>\n              ))}\n            </DropdownMenu>\n          </Dropdown>\n        </div>\n      )}\n    </div>\n  );\n};\nPagingHeader.propTypes = {\n  id: string,\n  paging: shape({\n    page: number.isRequired,\n    size: number.isRequired,\n    total: number.isRequired,\n  }).isRequired,\n  sorting: shape({\n    dir: oneOf(['asc', 'desc']),\n    options: arrayOf(\n      shape({\n        label: string.isRequired,\n        value: string.isRequired,\n        selected: bool, // only one should be selected\n      }).isRequired\n    ).isRequired,\n  }),\n  onSort: func,\n  type: string, // Type of items being paged, i.e. '1 - 10 of 42 <type>'\n};\n\nexport default PagingHeader;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/paging-header/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/sorting-header/SortingHeader.js",["335","336"],"import React from 'react';\nimport cx from 'classnames';\nimport { string, object, func } from 'prop-types';\nimport cookie from '../../util/cookie';\n\nimport styles from './styles';\n\n/**\n * Produces a table header with sorting indicator.\n */\nfunction SortingHeader({\n  property,\n  sorting: { by, dir },\n  onClick,\n  label,\n  saveAs,\n  className,\n  ...rest\n}) {\n  return (\n    <th\n      css={styles}\n      className={cx(className, 'sorting-header', {\n        active: by === property,\n      })}\n      onClick={e => {\n        onClick(e);\n        if (saveAs) {\n          cookie.set('sorting', saveAs, { by: property, dir });\n        }\n      }}\n      {...rest}\n    >\n      <i className={cx(`fa fa-angle-${dir === 'asc' ? 'up' : 'down'}`)} />{' '}\n      {label}\n    </th>\n  );\n}\nSortingHeader.propTypes = {\n  label: string.isRequired,\n  property: string.isRequired,\n  sorting: object.isRequired,\n  onClick: func.isRequired,\n  saveAs: string, // Cookie value to save choice\n};\n\nexport default SortingHeader;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/sorting-header/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/spinner/Spinner.js",["337"],"import React from 'react';\nimport cx from 'classnames';\n\n/**\n * Font-awesome notched circle outline that spins.\n */\nconst Spinner = ({ className = '', ...rest }) => (\n  <i className={cx(className, 'fa fa-circle-o-notch fa-spin')} {...rest} />\n);\n\nexport default Spinner;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/stepper/Stepper.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/stepper/styles.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/components/transitions/Fade.js",["338","339","340","341"],"import React, { Fragment } from 'react';\nimport { CSSTransition } from 'react-transition-group';\nimport cx from 'classnames';\nimport { Global } from '@emotion/core';\n\n/**\n * A css transition to do a fade in and out.\n * NOTE: using non-default timing (500 in, and 300 out) will require new css.\n * See https://reactcommunity.org/react-transition-group/css-transition\n */\nfunction Fade({ classNames, children, enter = 500, exit = 300, ...rest }) {\n  let timingClass = 'fade-in-out';\n  let timingStyles = null;\n\n  if (enter !== 500 || exit !== 300) {\n    // Define some custom fade timing\n    timingClass = `fade-in-out-${enter}-${exit}`;\n    timingStyles = (\n      <Global\n        styles={{\n          [`.${timingClass}-enter`]: { opacity: 0.01 },\n          [`.${timingClass}-enter.${timingClass}-enter-active`]: {\n            opacity: 1,\n            transition: `opacity ${enter}ms ease-in`,\n          },\n          [`.${timingClass}-exit`]: { opacity: 1 },\n          [`.${timingClass}-exit.${timingClass}-exit-active`]: {\n            opacity: 0.01,\n            transition: `opacity ${exit}ms ease-in`,\n          },\n        }}\n      />\n    );\n  }\n\n  return (\n    <Fragment>\n      {timingStyles}\n      <CSSTransition\n        classNames={cx(timingClass, classNames)}\n        timeout={{ enter, exit }}\n        {...rest}\n      >\n        {children}\n      </CSSTransition>\n    </Fragment>\n  );\n}\n\nexport default Fade;\n","/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/hooks/useHtmlId.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/hooks/usePrevious.js",[],"/Users/asolberg/Development/atsolberg/lwcc.org/src/scripts/hooks/useSizeChange.js",["342","343","344